# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET     /                           			controllers.SiteController.index()
GET     /certifications                         controllers.SiteController.certifications()
GET     /languages                           	controllers.SiteController.languages()
GET     /experience                           	controllers.SiteController.experience()
GET     /scholarship                           	controllers.SiteController.scholarship()
GET     /technologies                           controllers.SiteController.technologies()
GET     /contact                         		controllers.SiteController.contact()

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.at(path="/public", file)
